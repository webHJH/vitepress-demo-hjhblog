#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/rollup@3.29.3/node_modules/rollup/dist/bin/node_modules:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/rollup@3.29.3/node_modules/rollup/dist/node_modules:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/rollup@3.29.3/node_modules/rollup/node_modules:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/rollup@3.29.3/node_modules:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/rollup@3.29.3/node_modules/rollup/dist/bin/node_modules:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/rollup@3.29.3/node_modules/rollup/dist/node_modules:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/rollup@3.29.3/node_modules/rollup/node_modules:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/rollup@3.29.3/node_modules:/Users/qimao/Desktop/demo/vitepress-demo-hjhBlog/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@3.29.3/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@3.29.3/node_modules/rollup/dist/bin/rollup" "$@"
fi
